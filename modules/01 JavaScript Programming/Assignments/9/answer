var arr = new Array(100);

for(var i = 0; i < 100; i++)
{
    arr[i] = i + 1;
}

arr.forEach(function(element) {
    element = element * 2;
    console.log(element);
});

var names = ["David", "Yoni", "Guy", "Mariya", "Grisha"];

names.forEach(function(name){
    console.log("Hello",name);
});

for(var i = 1; i <= 5; i++)
{
    for(var j = 0; j < i; j++)
    {
        process.stdout.write("* ");
    }
    console.log();
}
