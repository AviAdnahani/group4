for (var index = 1; index <=100; index++) {
    if (index*2 <=100 && index*2 >=1)
    console.log(index*2);
}
var arr_names = ["david","shay","amir","ziv"];
arr_names.forEach(function(item)
{
    console.log("Hello",item);
});

var str = "";
for (var i = 1; i <= 5; i++) {
    for (var j = 1; j <= i; j++) {
           str = str+" *";
    }    
    console.log(str);
    str = "";
}
